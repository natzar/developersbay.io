<script type="text/x-handlebars" id="developers">
 <div class="row">
   
    
      <!-- left list column -->
      <div class="col-sm-5 bootcards-list" id="list" data-title="Contacts">
        <div class="panel panel-default">       
          <div class="panel-body">
            <div class="search-form">
              <div class="row">
                <div class="col-xs-12">
                  <div class="form-group">

					{{input class="input form-control"  type="text" value=filter placeholder="Filter developers"}}
						 <i class="fa fa-search"></i>                  
                   
                  </div>
                </div>
                
              </div>                
            </div>         
          </div>  <!--panel body-->

          <div class="list-group" style="height:400px;overflow:scroll;">  
			{{#each filteredContent}}
	        	{{#link-to 'developer' this tagName="a" class="list-group-item" }}
                {{imagepath src=image}}
                {{#if hot}}
                <span class="badge badge-success badge-info info">Available immediately</span>
                {{/if}}
	     	 	<h4 class="list-group-item-heading">{{name}}</h4>
	     	 	<p class="list-group-item-text">{{skills}}</p>
		        {{/link-to}}   
            {{/each}}
          </div><!--list-group-->

          <div class="panel-footer">
            <small class="pull-left">Showing {{filteredContent.length}} developers</small>

            
          </div>
        </div><!--panel-->

        </div><!--list-->

        <!--list details column-->
        <div class="col-sm-7 bootcards-cards hidden-xs">



         {{outlet}}

        </div><!--list-details-->

    </div><!--row-->

 </script>
 

 
 
 
 
 
