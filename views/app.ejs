<script type="text/x-handlebars" id="developer" data-template-name="developer">

    
         {{#if isEditing}}
         <div class="panel panel-default">
          <div class="panel-heading clearfix">
                <h3 class="panel-title pull-left">Edit {{model.name}}</h3>
<!--                <div class="btn-group pull-right visible-xs">
                 <button {{action 'edit'}}> 
                    <i class="fa fa-pencil"></i>
                    <span>Edit</span>
                  </button>
                </div>  
               <button classNames="btn btn-primary pull-right hidden-xs"  {{action 'edit'}}>
                  <i class="fa fa-pencil"></i>
                  <span>Edit</span>
                </button> -->
              </div>
              <div class="panel-body">
{{#object-form for=this  buttonLabel="Save"}}
 <label>Image Profile</label>

     


        <div class="buttons">
            <button class="btn" {{action "uploadAllFiles"}}>Upload All</button>
        </div>

        {{#if uploadStatus.uploading}}
            <h3 class="uploading-percentage">Uploaded Percentage: {{uploadStatus.percentComplete}}%</h3>
        {{/if}}

        {{#view view.DragDrop}}

            {{#if uploadStatus.error}}
                <div class="error">An error occurred during the upload process. Please try again later.</div>
            {{/if}}

            {{#each controller.validFiles}}

                <div {{bind-attr class="className :file"}}>
                    {{name}}
                    <a class="remove" {{action "deleteFile" this}}>Discard.</a>
                    {{view view.ImagePreview imageBinding="this"}}
                </div>

            {{/each}}

            {{view view.SingleInput}}

        {{/view}}
			{{form-field label="Name" for="name" value=model.name}}
			{{#form-field for="hot" label="Hot"}}
			{{input type="checkbox" class="form-control" checked=model.hot}}
			{{/form-field}}		
			{{#form-field for="description" label="Description"}}
			{{textarea class="form-control" value=model.description}}
			{{/form-field}}
			{{form-field label="Category" for="category" value=model.category}}
			{{form-field label="City" for="city" value=model.city}}
			{{form-field label="Country" for="country" value=model.country}}
			{{form-field label="Skills" for="Skills" value=model.skills}}	
			{{form-field label="Languages" for="Languages" value=model.languages}}			
			{{form-field label="Website" for="website" value=model.website}}			
			{{#form-field for="available" label="available"}}
			{{input type="checkbox" class="form-control" checked=model.available}}
			{{/form-field}}			
			{{form-field label="Email" for="email" value=model.email}}						
			{{/object-form}}
			</div>
        <div class="form-group">
      <button {{action 'doneEditing'}} class='btn btn-block btn-primary'>Guardar</button>
    </div>    
    </div>
    {{else}}
    
          <!--contact details -->
          <div id="contactCard">

            <div class="panel panel-default">
              <div class="panel-heading clearfix">
                <h3 class="panel-title pull-left">Developer Details </h3>
                <!-- <div class="btn-group pull-right visible-xs">
                 <button {{action 'edit'}}> 
                    <i class="fa fa-pencil"></i>
                    <span>Edit</span>
                  </button>
                </div>  
                <button classNames="btn btn-primary pull-right hidden-xs"  {{action 'edit'}}>
                  <i class="fa fa-pencil"></i>
                  <span>Edit</span>
                </button> -->
              </div>


              <div class="list-group">
                <div class="list-group-item">
                 {{imagepath src=model.image}}
                  <label>Name</label>
                  <h4 class="list-group-item-heading">{{model.name}} <small>{{model.birthDate}}</small></h4>

                </div>
				<div class="list-group-item">
              <label>Category</label>
                  <h4 class="list-group-item-heading">{{model.category}}</h4>
                </div>
                <div class="list-group-item">
                  <label>Expertise</label>
                  <h4 class="list-group-item-heading">{{model.skills}}</h4>
                </div>

                <div class="list-group-item">
                  <label>Location, Country</label>
                  <h4 class="list-group-item-heading">{{model.city}}, {{model.country}}</h4>
                </div>

                <div class="list-group-item">
                  <label>Languages</label>
                  <h4 class="list-group-item-heading">{{model.languages}}</h4>
                </div>

                <div class="list-group-item" >
                  <label>Available </label>
                  <h4 class="list-group-item-heading">{{model.available}}                   <small>Actualizado {{model.updated}}</small></h4>

                </div>
                <!--
                {{#if model.description}}
				<div class="list-group-item" >
                  <label>Notes</label>
                  <h4 class="list-group-item-heading h4-p" >{{model.description}}</h4>
                </div> 
                {{/if}}
                -->                
                {{#if model.trabajosAnteriores}}
                <div class="list-group-item" >
                  <label>Past Works</label>
                  <h4 class="list-group-item-heading" >{{model.trabajosAnteriores}} </h4>
                </div>
                {{/if}}                
<!--                <div class="list-group-item" >
                  <label>Prices</label>
                  <h4 class="list-group-item-heading">{{model.precios}} </h4>
                </div> -->
                {{#if model.redesSociales}}
                <div class="list-group-item" >
                  <label>Social Networks</label>
                  <h4 class="list-group-item-heading" >{{model.redesSociales}} </h4>
                </div>
                {{/if}}
                {{#if model.web}}
                <div class="list-group-item" >
                  <label>Personal Page</label>
                  <h4 class="list-group-item-heading" >{{model.web}} </h4>
                </div>
                {{/if}}
                {{#if model.email}}
                <div class="list-group-item">
                	<label>Contact</label>
                	


			{{#object-form for=this action="doneSendMessage" buttonLabel="Save"}}
  <div class="form-group">
  <label>Your Email</label>
   		{{input type=type value=from id=from placeholder="Your Email" class="form-control"}}
   		</div>
   		  <div class="form-group">
   		  <label>Your Message</label>
		{{textarea class="form-control" value=message  id=message  placeholder="Your Message"}}			

			</div>
{{/object-form}}
		<button {{action 'doneWritingMessage'}} class="btn btn-primary">Send Message</button>



		
                	<!-- <h4 class="list-group-item-heading">{{model.email}} </h4> -->
                	
                </div>
                {{/if}}				
              </div>

              <div class="panel-footer">
                <small class="pull-left">Developers Bay</small>

                </div>    
              </div>

            </div><!--contact card-->           
   {{/if}}
</script>
<script type="text/x-handlebars" id="adddeveloper">
	<div class="col-lg-12">	
		<div class="panel panel-default bootcards-richtext">
		<div class="panel-heading">
		<h3 class="panel-title">Add new developer to database</h3>
		</div>
		<div class="panel-body">
			{{#object-form for=this action="updateUser" buttonLabel="Save"}}
  <label>Image Profile</label>

     


        <div class="buttons">
            <button class="btn" {{action "uploadAllFiles"}}>Upload All</button>
        </div>

        {{#if uploadStatus.uploading}}
            <h3 class="uploading-percentage">Uploaded Percentage: {{uploadStatus.percentComplete}}%</h3>
        {{/if}}

        {{#view view.DragDrop}}

            {{#if uploadStatus.error}}
                <div class="error">An error occurred during the upload process. Please try again later.</div>
            {{/if}}

            {{#each controller.validFiles}}

                <div {{bind-attr class="className :file"}}>
                    {{name}}
                    <a class="remove" {{action "deleteFile" this}}>Discard.</a>
                    {{view view.ImagePreview imageBinding="this"}}
                </div>

            {{/each}}

            {{view view.SingleInput}}

        {{/view}}

			{{form-field label="Name" for="name" value=name}}
			{{#form-field for="hot" label="Hot"}}
			<small>Available immediately</small>
			{{input type="checkbox" class="form-control" value=1}}
			{{/form-field}}	
			{{form-field for="category" label="Category" value=category}}
			{{form-field for="skills" label="Skills" value=skills}}
			{{#form-field for="description" label="Description"}}
			{{textarea class="form-control" value=description}}
			{{/form-field}}
			{{form-field label="City" for="city" value=city}}
			{{form-field label="Country" for="country" value=country}}
			{{form-field label="Languages" for="Languages" value=languages}}			
			{{#form-field for="available" label="Available" value=available}}
			{{input type="checkbox" class="form-control" value=1}}
			{{/form-field}}			
			{{form-field label="Email" for="email" value=email}}						
			{{/object-form}}
		</div>
		<div class="panel-footer">
		<button {{action 'doneCreating'}} class="btn btn-primary">Guardar</button>
		</div>
		</div><!--end rich text card--> 


      </div>      
</script>
 
  <script type="text/x-handlebars" data-template-name="components/image">

